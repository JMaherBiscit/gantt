{
	"name": "wx-svelte-gantt",
	"version": "1.2.0",
	"productTag": "gantt",
	"needsTrial": true,
	"type": "module",
	"scripts": {
		"build": "vite build",
		"build:tests": "vite build --mode test",
		"lint": "yarn eslint ./demos ./src --ext .svelte,.ts,.js",
		"start": "vite --open",
		"start:tests": "vite --open=/tests/ --mode test --host 0.0.0.0  --port 5100",
		"test": "true",
		"test:cypress": "cypress run -P ./ --config \"baseUrl=http://localhost:5100/tests\""
	},
	"svelte": "src/index.js",
	"exports": {
		".": {
			"svelte": "./src/index.js"
		},
		"./package.json": "./package.json"
	},
	"license": "GPL-3.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/svar-widgets/gantt.git"
	},
	"bugs": {
		"url": "https://forum.svar.dev"
	},
	"homepage": "https://svar.dev/svelte/gantt/",
	"dependencies": {
		"wx-gantt-data-provider": "1.2.0",
		"wx-gantt-locales": "1.2.0",
		"wx-gantt-store": "1.2.0",
		"wx-lib-dom": "0.6.0",
		"wx-lib-state": "1.9.0",
		"wx-lib-svelte": "0.4.0",
		"wx-svelte-core": "1.3.0",
		"wx-svelte-menu": "1.3.0",
		"wx-svelte-grid": "1.3.0",
		"wx-svelte-toolbar": "1.3.0"
	},
	"files": [
		"src",
		"readme.md",
		"whatsnew.md",
		"license.txt"
	]
}
